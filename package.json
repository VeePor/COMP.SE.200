{
  "name": "comp.se.200",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:human": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=tests/human",
    "test:ai": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=tests/ai",
    "coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "coverage:human": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --testPathPattern=tests/human",
    "coverage:ai": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage --testPathPattern=tests/ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VeePor/COMP.SE.200.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VeePor/COMP.SE.200/issues"
  },
  "homepage": "https://github.com/VeePor/COMP.SE.200#readme",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-html-reporter": "^4.3.0"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/.internal/**"
    ],
    "reporters": [
      "default",
      [
        "jest-html-reporter",
        {
          "pageTitle": "Test Report",
          "outputPath": "jest-report.html",
          "includeFailureMsg": true,
          "includeConsoleLog": true
        }
      ]
    ]
  }
}
